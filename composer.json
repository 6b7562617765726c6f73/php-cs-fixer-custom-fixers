{
    "name": "kubawerlos/php-cs-fixer-custom-fixers",
    "description": "A set of custom fixers for PHP CS Fixer",
    "type": "library",
    "keywords": [
        "fixer",
        "php-cs-fixer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kuba Wer≈Ços",
            "email": "werlos@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-tokenizer": "*",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "require-dev": {
        "kubawerlos/types-checker": "^1.1",
        "phpunit/phpunit": "^7.1",
        "sebastian/diff": "^3.0",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/yaml": "^4.0",
        "vimeo/psalm": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PhpCsFixerCustomFixers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
